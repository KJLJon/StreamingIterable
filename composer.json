{
    "name": "kjljon/streamingiterable",
    "description": "Convert iteratables into with streams using psr-7 without any other dependencies",
    "keywords": [
        "streams",
        "psr-7",
        "psr7",
        "iterable",
        "array",
        "Iterator",
        "Generator",
        "csv",
        "json"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Lietzau",
            "email": "streamingiterable@kjljon.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-message": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KJLJon\\StreamingIterable\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "KJLJon\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit --configuration phpunit.xml"
        ]
    }
}
